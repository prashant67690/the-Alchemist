<%- include("partials/header") -%>

<h1>Compose</h1>

<form  action="/compose" method="post">
  <div class="form-group">
    <label for="inputdefault">Title</label>
    <input name="postTitle"class="form-control" id="inputdefault" type="text" autocomplete="off">
  </div>
  <div class="form-group">
    <label for="inputdefault">Post</label>
    <textarea class="form-control" name="postBody" rows="8" cols="80"></textarea>
  </div>
  <br>
  <button class="btn btn-primary" type="submit" name="publish">Publish</button>
</form>

<%- include("partials/footer") -%>
